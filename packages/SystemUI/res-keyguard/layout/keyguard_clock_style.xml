<?xml version="1.0" encoding="utf-8"?>
<com.android.systemui.afterlife.ClockStyle
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <LinearLayout
        android:gravity="start"
        android:id="@+id/keyguard_clock_style_oos"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">
        <include layout="@layout/keyguard_clock_oos" />
    </LinearLayout>
    <LinearLayout
        android:gravity="center"
        android:id="@+id/keyguard_clock_style_ios"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">
        <include layout="@layout/keyguard_clock_ios" />
    </LinearLayout>
    <LinearLayout
        android:gravity="center"
        android:id="@+id/keyguard_clock_style_cos"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">
        <include layout="@layout/keyguard_clock_cos" />
    </LinearLayout>
    <LinearLayout
        android:gravity="center"
        android:id="@+id/keyguard_clock_style_custom"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">
        <include layout="@layout/keyguard_clock_custom" />
    </LinearLayout>
    <LinearLayout
        android:gravity="center"
        android:id="@+id/keyguard_clock_style_custom1"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">
        <include layout="@layout/keyguard_clock_custom_1" />
    </LinearLayout>
    <LinearLayout
        android:gravity="center"
        android:id="@+id/keyguard_clock_style_custom2"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">
        <include layout="@layout/keyguard_clock_custom_2" />
    </LinearLayout>
    <LinearLayout
        android:gravity="center|top"
        android:id="@+id/keyguard_clock_style_custom3"
        android:layout_width="fill_parent"
        android:layout_height="320.0dip">
        <include layout="@layout/keyguard_clock_custom_3" />
    </LinearLayout>
</com.android.systemui.afterlife.ClockStyle>