    <java-symbol type="string" name="alert_slider_mode_normal" />
    <java-symbol type="string" name="alert_slider_mode_priority" />
    <java-symbol type="string" name="alert_slider_mode_vibrate" />
    <java-symbol type="string" name="alert_slider_mode_silent" />
    <java-symbol type="string" name="alert_slider_mode_dnd" />

